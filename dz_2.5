number = int(input('Введите число - '))

list = [7, 5, 3, 3, 2]

if number > 7:
    list.insert(0, number)
    print(list)
elif number > 5 and number <= 7:
    list.insert(1, number)
    print(list)
elif number <= 5 or number == 4:
    list.insert(2, number)
    print(list)
elif number == 3:
    list.insert(4, number)
    print(list)
elif number <= 2:
    list.append(number)
    print(list)
else:
    print("Ошибка")